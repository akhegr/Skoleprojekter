<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobilSemProjekt.View.ProfilePage"
             Title="Profil">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Orientation="Vertical">
                <Button x:Name="SeeMyLocationsButton" Text="See my locations" Clicked="SeeMyLocationsButton_OnClicked" HorizontalOptions="CenterAndExpand"/>
                <Button x:Name="SeeMyRatingsButton" Text="See my ratings" Clicked="SeeMyRatingsButton_OnClicked" HorizontalOptions="CenterAndExpand"/>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>